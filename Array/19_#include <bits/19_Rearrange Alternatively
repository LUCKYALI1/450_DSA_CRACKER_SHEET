#include <bits/stdc++.h> 

void rearrange(vector<int> &arr)
{
    vector<int> pos;
	vector<int> neg;
	for(int i = 0; i < arr.size() ; i++){
		if(arr[i] < 0){
			neg.push_back(arr[i]);
			
		}
		else{
			pos.push_back(arr[i]);
		}
	}
	int i = 0;
	int p = 0; 
	int n = 0; 
	while (p < pos.size() && n < neg.size()) {
		arr[i++] = pos[p++];
		arr[i++] = neg[n++];
	}
	while (p < pos.size()) {
		arr[i++] = pos[p++];
	}

	while (n < neg.size()) {
		arr[i++] = neg[n++];
	}
      
}
