class Solution {
  public:
    // Function to check if a string is a palindrome.
    bool isPalindrome(string& s) {
       int l = 0;
       int h = s.size()-1;
       while(l<h){
           if(s[l]==s[h]){
               l++;
               h--;
           }
           else{
               return false;
           }
       }
       return true;
    }
};
